{
    "name": "golem",
    "version": "0.1.8",
    "description": "Worldwide supercomputer",
    "private": true,
    "keywords": [
        "Golem, Super Computer, Render, Blender, Blockchain, Ethereum, LuxRender, Cryptocurrency, GNT"
    ],
    "author": {
        "name": "Golem Factory",
        "email": "muhammed@golem.network"
    },
    "license": "GPL-3.0",
    "main": "index.js",
    "scripts": {
        "postinstall": "electron-builder install-app-deps",
        "compile": "webpack --config webpack.config.prod.babel.js",
        "start:app": "node server.js",
        "start": "cross-env NODE_ENV=development electron .",
        "start:prod": "electron .",
        "test": "jest -u",
        "test:watch": "jest --watch --no-cache",
        "test:coverage": "jest --coverage --watch --no-cache",
        "pack": "build --dir",
        "dist": "build"
    },
    "build": {
        "appId": "network.golem.app",
        "mac": {
            "category": "public.app-category.productivity"
        },
        "files": [
            "index.js",
            "electron/",
            "app/",
            "index.html",
            "index.frame.html",
            "index.documentation.html",
            "!coverage/*",
            "!index.yedek.html",
            "!template.html",
            "!setup_jest.js",
            "!setup_golem.js",
            "!server.js",
            "!yarn.lock",
            "!yarn-error.log",
            "!webpack.config.babel.js",
            "!webpack.config.prod.babel.js",
            "!package-lock.json",
            "!.gitignore",
            "!.babelrc",
            "!HyperG.db",
            "!src/*",
            "!logs/*",
            "!dist/*",
            "!build/*",
            "!__mocks__/*",
            "!app/*.gz",
            "!node_modules/",
            "node_modules/**/*/{LICENSE}",
            "node_modules/winston-uber/",
            "node_modules/chalk/",
            "node_modules/escape-string-regexp/",
            "node_modules/ansi-styles/",
            "node_modules/strip-ansi/",
            "node_modules/supports-color/",
            "node_modules/color-convert/",
            "node_modules/has-flag/",
            "node_modules/has-ansi/",
            "node_modules/ansi-regex/",
            "node_modules/mkdirp/",
            "node_modules/pkginfo/",
            "node_modules/cycle",
            "node_modules/stack-trace",
            "node_modules/request",
            "node_modules/eyes",
            "node_modules/colors",
            "node_modules/async",
            "node_modules/electron-store",
            "node_modules/conf",
            "node_modules/is-obj",
            "node_modules/make-dir",
            "node_modules/pify",
            "node_modules/locate-path",
            "node_modules/p-locate",
            "node_modules/p-limit",
            "node_modules/imurmurhash",
            "node_modules/signal-exit",
            "node_modules/dot-prop",
            "node_modules/pkg-up",
            "node_modules/find-up",
            "node_modules/path-exists",
            "node_modules/env-paths",
            "node_modules/write-file-atomic",
            "node_modules/graceful-fs",
            "node_modules/semver",
            "node_modules/p-try"
        ]
    },
    "devDependencies": {
        "app-module-path": "1.1.0",
        "babel-core": "^6.26.0",
        "babel-jest": "^19.0.0",
        "babel-loader": "7.1.2",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "case-sensitive-paths-webpack-plugin": "1.1.2",
        "compression-webpack-plugin": "^0.3.2",
        "cross-env": "^5.0.1",
        "css-loader": "^0.26.1",
        "electron": "^1.7.12",
        "electron-builder": "^19.47.1",
        "electron-debug": "^1.2.0",
        "electron-devtools-installer": "^2.2.1",
        "electron-reload": "^1.2.2",
        "enzyme": "^3.2.0",
        "enzyme-adapter-react-16": "^1.1.0",
        "enzyme-to-json": "^3.2.2",
        "express": "^4.14.0",
        "file-loader": "^0.9.0",
        "html-webpack-harddisk-plugin": "^0.1.0",
        "html-webpack-plugin": "^2.28.0",
        "http": "0.0.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^21.2.1",
        "mock-socket": "^6.0.4",
        "node-sass": "^4.7.2",
        "react-a11y": "^0.3.3",
        "react-hot-loader": "^3.1.3",
        "redux-mock-store": "^1.2.2",
        "redux-saga-test-plan": "^3.3.1",
        "regenerator-runtime": "^0.11.0",
        "sass-loader": "^6.0.6",
        "sinon": "^1.17.7",
        "style-loader": "^0.13.1",
        "url": "^0.11.0",
        "webpack": "^3.9.1",
        "webpack-bundle-analyzer": "^2.3.0",
        "webpack-dev-middleware": "^1.12.2",
        "webpack-hot-middleware": "^2.13.2"
    },
    "dependencies": {
        "axios": "^0.15.3",
        "chalk": "^1.1.3",
        "createjs-browserify": "^1.0.5",
        "css-browser-selector": "^0.6.5",
        "electron-store": "^1.3.0",
        "msgpack5": "^3.4.1",
        "openseadragon": "^2.3.1",
        "rc-tooltip": "^3.4.2",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-motion": "^0.5.2",
        "react-redux": "^5.0.6",
        "react-router": "^3.2.0",
        "react-router-redux": "^4.0.7",
        "react-transition-group": "^1.2.0",
        "redux": "^3.7.0",
        "redux-saga": "0.16.0",
        "semver": "^5.4.1",
        "timepoint-selection": "git+https://github.com/mdtanrikulu/timepoint-selection.git#maximum_date_normalization",
        "uuid": "^3.0.1",
        "wampy": "^4.1.0",
        "websocket": "^1.0.24",
        "winston-uber": "^1.0.1",
        "yup": "^0.23.0"
    },
    "jest": {
        "setupFiles": [
            "./setup_jest.js"
        ],
        "snapshotSerializers": [
            "./node_modules/enzyme-to-json/serializer"
        ],
        "moduleNameMapper": {
            "^.+\\.(css|scss)$": "identity-obj-proxy",
            "^.*[.](png|PNG|svg|SVG)$": "<rootDir>/__mocks__/EmptyModule"
        },
        "transformIgnorePatterns": [
            "/node_modules/"
        ],
        "unmockedModulePathPatterns": [
            "<rootDir>/node_modules/react",
            "<rootDir>/node_modules/react-dom",
            "<rootDir>/node_modules/fbjs",
            "<rootDir>/node_modules/react-addons-test-utils",
            "<rootDir>/__mocks__/EmptyModule.js"
        ]
    },
    "env": {
        "test": {
            "plugins": [
                "transform-es2015-modules-commonjs"
            ]
        }
    }
}
