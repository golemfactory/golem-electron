{
    "name": "golem",
    "version": "0.1.18",
    "description": "Worldwide supercomputer",
    "private": true,
    "keywords": [
        "Golem, Super Computer, Render, Blender, Blockchain, Ethereum, LuxRender, Cryptocurrency, GNT"
    ],
    "author": {
        "name": "Golem Factory",
        "email": "muhammed@golem.network"
    },
    "license": "GPL-3.0",
    "main": "index.js",
    "scripts": {
        "postinstall": "electron-builder install-app-deps",
        "compile": "webpack --mode production --config webpack.config.babel.js",
        "start:app": "webpack-dev-server --mode development --hot --port 3002",
        "start": "cross-env NODE_ENV=development electron .",
        "start:mainnet": "cross-env NODE_ENV=development electron . --mainnet",
        "start:prod": "cross-env NODE_ENV=production electron .",
        "test": "jest -u",
        "test:watch": "jest --watch --no-cache",
        "test:coverage": "jest --coverage --watch --no-cache",
        "pack": "build --dir",
        "dist": "build",
        "dist:all": "build -mwl",
        "dist:debug": "cross-env DEBUG=electron-builder build"
    },
    "build": {
        "appId": "network.golem.app",
        "mac": {
            "category": "public.app-category.productivity",
            "target": "dmg",
            "artifactName": "golem-electron-beta-${os}os-${version}.${ext}"
        },
        "win": {
            "target": "zip",
            "publisherName": "Golem Factory GmbH",
            "artifactName": "golem-electron-beta-${os}-${version}.${ext}"
        },
        "linux": {
            "target": "tar.gz",
            "icon": "./build/icons/32x32.png",
            "category": "Utility",
            "artifactName": "golem-electron-beta-${os}-${version}.${ext}"
        },
        "files": [
            "index.js",
            "electron/",
            "app/",
            "index.html",
            "index.frame.html",
            "index.documentation.html",
            "!coverage/*",
            "!template.html",
            "!setup_jest.js",
            "!setup_golem.js",
            "!yarn.lock",
            "!yarn-error.log",
            "!webpack.config.babel.js",
            "!package-lock.json",
            "!.gitignore",
            "!.babelrc",
            "!HyperG.db",
            "!src/*",
            "!logs/*",
            "!dist/*",
            "!build/*",
            "!__mocks__/*",
            "!app/*.gz",
            "node_modules/**/LICENSE",
            "node_modules/**/*LICENSE*",
            "node_modules/**/*LICENCE*",
            "node_modules/**/License",
            "node_modules/**/license",
            "!node_modules/**/*.pdb",
            "!node_modules/**/*.o.d",
            "!node_modules/**/*.a.d",
            "!node_modules/**/*.obj",
            "!node_modules/**/*.lib",
            "!node_modules/**/*.exp",
            "!node_modules/**/MAKEFILE",
            "!node_modules/**/Makefile",
            "!node_modules/**/*.Makefile",
            "!node_modules/**/*.target.mk",
            "!node_modules/**/*.node.mk",
            "!node_modules/**/Cakefile",
            "!node_modules/**/Gruntfile.js",
            "!node_modules/**/Vagrantfile",
            "!node_modules/**/.tern-port",
            "!node_modules/**/*.cc",
            "!node_modules/**/*.c",
            "!node_modules/**/*.s",
            "!node_modules/**/*.S",
            "!node_modules/**/*.py",
            "!node_modules/**/*.pyc",
            "!node_modules/**/*.pl",
            "!node_modules/**/*.rb",
            "!node_modules/**/*.tlog",
            "!node_modules/**/*.sln",
            "!node_modules/**/.jshintrc",
            "!node_modules/**/.lint",
            "!node_modules/**/jsl.node.conf",
            "!node_modules/**/.eslintrc",
            "!node_modules/**/.editorconfig",
            "!node_modules/**/.jscs.json",
            "!node_modules/**/.npmignore",
            "!node_modules/**/.eslintignore",
            "!node_modules/**/*.dntrc",
            "!node_modules/**/*.cpp",
            "!node_modules/**/*.jpg",
            "!node_modules/**/*.png",
            "!node_modules/**/.gitmodules",
            "!node_modules/**/*.h",
            "!node_modules/**/*.patch",
            "!node_modules/**/_test",
            "!node_modules/**/README",
            "!node_modules/**/README.md",
            "!node_modules/**/usage.txt",
            "!node_modules/**/*.md",
            "!node_modules/**/*.txt",
            "!node_modules/**/*.markdown",
            "!node_modules/**/Changelog",
            "!node_modules/**/CHANGES",
            "!node_modules/**/CHANGELOG",
            "!node_modules/**/.travis.yml",
            "!node_modules/**/appveyor.yml",
            "!node_modules/**/robot.html",
            "!node_modules/**/examples",
            "!node_modules/**/example",
            "!node_modules/**/example.js",
            "!node_modules/**/screenshots",
            "!node_modules/**/.gitattributes",
            "!node_modules/**/benchmarks",
            "!node_modules/**/*benchmark*",
            "!node_modules/**/coverage",
            "!node_modules/**/docs",
            "!node_modules/**/*.coffee",
            "!node_modules/**/tests",
            "!node_modules/**/*.vcxproj",
            "!node_modules/**/*.vcxproj.filters",
            "!node_modules/**/node-pre-gyp",
            "!node_modules/**/node-gyp",
            "!node_modules/**/gyp",
            "!node_modules/**/*.gypi"
        ]
    },
    "devDependencies": {
        "app-module-path": "1.1.0",
        "babel-core": "^6.26.0",
        "babel-jest": "^19.0.0",
        "babel-loader": "7.1.4",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "bignumber.js": "7.2.1",
        "case-sensitive-paths-webpack-plugin": "2.1.2",
        "compression-webpack-plugin": "^1.1.11",
        "connected-react-router": "^4.3.0",
        "createjs-browserify": "^1.0.5",
        "cross-env": "^5.0.1",
        "css-browser-selector": "^0.6.5",
        "css-loader": "2.1.0",
        "electron": "2.0.17",
        "electron-builder": "20.38.5",
        "electron-debug": "^1.2.0",
        "electron-devtools-installer": "2.2.4",
        "electron-reload": "^1.2.2",
        "enzyme": "3.6.0",
        "enzyme-adapter-react-16": "1.7.0",
        "enzyme-to-json": "3.3.4",
        "express": "^4.14.0",
        "fibers": "^3.1.1",
        "file-loader": "1.1.11",
        "handlebars": "^4.0.11",
        "happypack": "^5.0.0",
        "history": "^4.7.2",
        "html-webpack-harddisk-plugin": "0.2.0",
        "html-webpack-plugin": "^3.2.0",
        "html2pdf.js": "^0.9.0",
        "http": "0.0.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "23.4.1",
        "jest-canvas-mock": "^1.1.0",
        "mock-socket": "^6.0.4",
        "msgpack5": "^3.4.1",
        "openseadragon": "^2.3.1",
        "qrcode-generator": "^1.3.1",
        "re-reselect": "^2.1.0",
        "react": "16.4.2",
        "react-a11y": "1.0.0",
        "react-dom": "16.4.2",
        "react-hot-loader": "4.5.1",
        "react-password-strength": "^2.3.1",
        "react-pose": "^3.3.5",
        "react-redux": "^5.0.7",
        "react-router-dom": "^4.3.1",
        "react-router-redux": "^4.0.7",
        "react-spring": "7.2.9",
        "react-tippy": "github:mdtanrikulu/react-tippy#svg_support",
        "react-transition-group": "^1.2.1",
        "react-virtualized": "^9.20.1",
        "redux": "4.0.0",
        "redux-actions-assertions": "^1.3.0",
        "redux-mock-store": "1.5.3",
        "redux-saga": "0.16.0",
        "redux-saga-test-plan": "^3.3.1",
        "regenerator-runtime": "^0.11.0",
        "reselect": "^3.0.1",
        "sass": "^1.16.1",
        "sass-loader": "7.1.0",
        "sinon": "^1.17.7",
        "style-loader": "^0.13.1",
        "timepoint-selection": "git+https://github.com/mdtanrikulu/timepoint-selection.git#maximum_date_normalization",
        "url": "^0.11.0",
        "uuid": "^3.0.1",
        "wampy": "^6.2.0",
        "webpack": "4.29.0",
        "webpack-bundle-analyzer": "^2.3.0",
        "webpack-cli": "^3.0.8",
        "webpack-dev-server": "^3.1.14",
        "websocket": "^1.0.24",
        "yup": "^0.23.0"
    },
    "dependencies": {
        "axios": "0.18.0",
        "chalk": "2.4.1",
        "electron-store": "2.0.0",
        "lodash.isequal": "^4.5.0",
        "mkdirp": "^0.5.1",
        "react-lottie": "^1.2.3",
        "semver": "^5.4.1",
        "wampy-cra": "^0.1.1",
        "web3-net": "^1.0.0-beta.37",
        "web3-utils": "^1.0.0-beta.37",
        "winston-uber": "^1.0.1",
        "yargs": "^12.0.2"
    },
    "jest": {
        "setupFiles": [
            "./setup_jest.js",
            "jest-canvas-mock"
        ],
        "snapshotSerializers": [
            "./node_modules/enzyme-to-json/serializer"
        ],
        "moduleNameMapper": {
            "^.+\\.(css|scss)$": "identity-obj-proxy",
            "^.*[.](png|PNG|svg|SVG)$": "<rootDir>/__mocks__/EmptyModule"
        },
        "transformIgnorePatterns": [
            "/node_modules/"
        ],
        "unmockedModulePathPatterns": [
            "<rootDir>/node_modules/react",
            "<rootDir>/node_modules/react-dom",
            "<rootDir>/node_modules/fbjs",
            "<rootDir>/node_modules/react-addons-test-utils",
            "<rootDir>/__mocks__/EmptyModule.js"
        ]
    },
    "env": {
        "test": {
            "plugins": [
                "transform-es2015-modules-commonjs"
            ]
        }
    }
}
