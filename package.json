{
    "name": "golem",
    "version": "0.1.3",
    "description": "Worldwide supercomputer",
    "private": true,
    "keywords": [
        "Golem, Super Computer, Render, Blender, Blockchain, Ethereum, LuxRender, Cryptocurrency, GNT"
    ],
    "author": {
        "name": "Muhammed TanrÄ±kulu",
        "email": "md.tanrikulu@gmail.com"
    },
    "license": "GPL-3.0",
    "main": "index.js",
    "scripts": {
        "postinstall": "electron-builder install-app-deps",
        "compile": "webpack --config webpack.config.prod.babel.js",
        "start:app": "node server.js",
        "start": "cross-env NODE_ENV=development electron .",
        "start:prod": "electron .",
        "test": "jest",
        "test:ci": "jest --coverage",
        "test:watch": "jest --watch --no-cache",
        "test:coverage": "jest --coverage --watch --no-cache",
        "pack": "build --dir",
        "dist": "build"
    },
    "build": {
        "appId": "network.golem.app",
        "mac": {
            "category": "public.app-category.productivity"
        },
        "files": [
            "!coverage/*",
            "!index.yedek.html",
            "!template.html",
            "!setup_jest.js",
            "!yarn.lock",
            "!yarn-error.log",
            "!webpack.config.babel.js",
            "!webpack.config.prod.babel.js",
            "!HyperG.db",
            "!logs/*",
            "!dist/*",
            "!build/*",
            "!__mocks__/*",
            "!app/*.gz",
            "!node_modules/axios",
            "!node_modules/createjs-browserify",
            "!node_modules/msgpack5",
            "!node_modules/openseadragon",
            "!node_modules/rc-tooltip",
            "!node_modules/react",
            "!node_modules/react-dom",
            "!node_modules/redux",
            "!node_modules/react-motion",
            "!node_modules/react-redux",
            "!node_modules/react-a11y",
            "!node_modules/react-router",
            "!node_modules/react-router-redux",
            "!node_modules/redux-saga",
            "!node_modules/resumablejs",
            "!node_modules/timepoint-selection",
            "!node_modules/wampy",
            "!node_modules/websocket"
        ]
    },
    "devDependencies": {
        "app-module-path": "1.1.0",
        "babel-core": "^6.18.2",
        "babel-jest": "^19.0.0",
        "babel-loader": "^6.2.8",
        "babel-plugin-transform-class-properties": "^6.22.0",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
        "babel-polyfill": "^6.22.0",
        "babel-preset-es2015": "^6.18.0",
        "babel-preset-react": "^6.16.0",
        "babel-preset-stage-0": "^6.16.0",
        "case-sensitive-paths-webpack-plugin": "1.1.2",
        "compression-webpack-plugin": "^0.3.2",
        "cross-env": "^5.0.1",
        "css-loader": "^0.26.1",
        "electron": "^1.4.5",
        "electron-builder": "^19.5.1",
        "electron-debug": "^1.2.0",
        "electron-devtools-installer": "2.0.1",
        "electron-rebuild": "^1.5.7",
        "enzyme": "^2.7.1",
        "enzyme-to-json": "^1.4.5",
        "express": "^4.14.0",
        "file-loader": "^0.9.0",
        "html-webpack-harddisk-plugin": "^0.1.0",
        "html-webpack-plugin": "^2.28.0",
        "http": "0.0.0",
        "identity-obj-proxy": "^3.0.0",
        "jest-cli": "^18.1.0",
        "mock-socket": "^6.0.4",
        "node-sass": "^3.13.0",
        "react-a11y": "^0.3.3",
        "react-addons-test-utils": "^15.4.2",
        "react-hot-loader": "^3.0.0-beta.6",
        "redux-mock-store": "^1.2.2",
        "redux-saga-test-plan": "^2.2.0",
        "sass-loader": "^4.0.2",
        "sinon": "^1.17.7",
        "style-loader": "^0.13.1",
        "url": "^0.11.0",
        "webpack": "^2.2.0",
        "webpack-bundle-analyzer": "^2.3.0",
        "webpack-dev-middleware": "^1.8.4",
        "webpack-hot-middleware": "^2.13.2"
    },
    "dependencies": {
        "axios": "^0.15.3",
        "chalk": "^1.1.3",
        "createjs-browserify": "^1.0.5",
        "css-browser-selector": "^0.6.5",
        "electron-config": "^0.2.1",
        "getpid": "0.0.1",
        "history": "^4.6.2",
        "msgpack5": "^3.4.1",
        "openseadragon": "^2.2.1",
        "promise": "7.1.1",
        "rc-tooltip": "^3.4.2",
        "react": "^15.4.1",
        "react-dom": "^15.4.1",
        "react-motion": "^0.4.7",
        "react-redux": "^5.0.0",
        "react-router": "^3.0.0",
        "react-router-redux": "^4.0.7",
        "redux": "^3.6.0",
        "redux-saga": "0.14.7",
        "request": "^2.81.0",
        "request-progress": "^3.0.0",
        "resumablejs": "^1.0.2",
        "serve-static": "^1.12.3",
        "timepoint-selection": "^0.2.2",
        "unzip": "^0.1.11",
        "uuid": "^3.0.1",
        "wampy": "^4.1.0",
        "websocket": "^1.0.24"
    },
    "jest": {
        "setupFiles": [
            "./setup_jest.js"
        ],
        "snapshotSerializers": [
            "./node_modules/enzyme-to-json/serializer"
        ],
        "moduleNameMapper": {
            "^.+\\.(css|scss)$": "identity-obj-proxy",
            "^.*[.](png|PNG)$": "<rootDir>/__mocks__/EmptyModule"
        },
        "transformIgnorePatterns": [
            "/node_modules/"
        ],
        "unmockedModulePathPatterns": [
            "<rootDir>/node_modules/react",
            "<rootDir>/node_modules/react-dom",
            "<rootDir>/node_modules/react-addons-test-utils",
            "<rootDir>/__mocks__/EmptyModule.js"
        ]
    },
    "env": {
        "test": {
            "plugins": [
                "transform-es2015-modules-commonjs"
            ]
        }
    }
}
